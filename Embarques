	<h2>Valoracion de Embarques</h2>


	<p id="demo">embarque no rentable</p>
	<p id="nombre">Inteligencia de Negocios</p>

	<script>
// dias de entrega de los embarques
var entrega = 3;
//  dias de espera para la entrega (horizonte de planeaciom)   
var ho = 3;
// comparacion de los datos
if (ho>entrega){
		// si los dias de entrega son menores a los dias de planeacion entonces el embarque es rentable
		document.getElementById("demo").innerHTML = "embarque rentable";
		
	}
	else {
	   // en caso contrario no es rentable ya que el embarque llegara en una situaciï¿½n critica
	   document.getElementById("demo").innerHTML = "embarque no rentable";
	}
	
	document.getElementById("nombre").innerHTML = "Inteligencia de Negocios";
</script>


</body></html>
